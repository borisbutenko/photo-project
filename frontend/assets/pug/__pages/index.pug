extends ../base
block main
  section.section__main
    .container-fluid
      .row
        .col-md-2.col-sm-12.col-xs-12
          .left-content
            .content-box.gift
              h2 Мои подарки
              .gift__last.clearfix
                | Подарок
                .gift__date.pull-left 12/17
                .gift__time.pull-right 7:32
                img(src='assets/img/bottom-banner/1.jpg' alt='')
                a.btn.btn-default(href='#0')
                  span Принять участие
              h2 Подарки отправили
              ul.gift__list
                li.gift__item
                  a.gift__link(href='#0')
                    img(src='assets/img/bottom-banner/1.jpg' alt='')
                    .gift__text
                      | Москва<br>
                      | Сема
                li.gift__item
                  a.gift__link(href='#0')
                    img(src='assets/img/bottom-banner/1.jpg' alt='')
                    .gift__text
                      | Москва<br>
                      | Сема
                li.gift__item
                  a.gift__link(href='#0')
                    img(src='assets/img/bottom-banner/1.jpg' alt='')
                    .gift__text
                      | Москва<br>
                      | Сема
              p.gift__count Всего подарков: 18
              h2 Страны доставки
              ul.gift__country
                - var n = 0
                while n < 10
                  li
                    a(href='#0')
                      img(src='assets/img/bottom-banner/1.jpg' alt='')
                  - n++
